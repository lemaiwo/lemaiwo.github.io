<mvc:View controllerName="be.wl.lemtech.wouter.controller.AboutMe"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.f">
   <f:DynamicPage id="homepage" showFooter="false" class="sapUiNoContentPadding">
        <f:content>
            <l:Grid
                    defaultSpan="XL10 L10 M12 S12"
                    hSpacing="0"
                    vSpacing="0">
                    <VBox alignItems="Center">
                        <layoutData>
                            <l:GridData span="XL1 L1 M12 S12"/>
                        </layoutData>
                    </VBox>
            <l:BlockLayout>
                <l:BlockLayoutRow>
                    <l:BlockLayoutCell
                            backgroundColorSet="ColorSet10"
                            backgroundColorShade="ShadeB"
                            title="">
                            <VBox alignItems="Center">
                                <Avatar src="images/image002.jpg" displaySize="Custom" customDisplaySize="20rem" press=".onAvatarPressed" displayShape="Circle" showBorder="true" tooltip="Profile">
                                    <detailBox>
                                        <LightBox>
                                            <LightBoxItem
                                                    imageSrc="images/image002.jpg"
                                                    alt="Wouter Lemaire"
                                                    title="Wouter Lemaire"
                                                    subtitle="SAP Solution Architect, Developer &amp; part of SAP Mentors">
                                            </LightBoxItem>
                                        </LightBox>
                                    </detailBox>
                                    <!-- <layoutData>
                                        <OverflowToolbarLayoutData priority="NeverOverflow"/>
                                    </layoutData> -->
                                </Avatar>
                                <Title width="100%"
                                            level="H5"
                                            titleStyle="H5"
                                            text="Follow me on:"
                                            class="sapUiSmallMarginTopBottom">
                                        </Title>
                                <HBox >
                                    <Image src="./dist/images/github.png" width="50px" press=".onOpenFollowMeOn('https://github.com/lemaiwo')" class="sapUiTinyMarginBegin sapUiSmallMarginEnd"></Image>
                                    <Image src="./dist/images/xlogoblack.png" width="50px" press=".onOpenFollowMeOn('https://twitter.com/wouter_lemaire')" class="sapUiTinyMarginBegin sapUiSmallMarginEnd"></Image>
                                    <Image src="./dist/images/linkedin.png" width="50px" press=".onOpenFollowMeOn('https://www.linkedin.com/in/wouterlemaire')" class="sapUiTinyMarginBegin sapUiSmallMarginEnd"></Image>
                                    <Image src="./dist/images/sap.png" width="100px" press=".onOpenFollowMeOn('https://community.sap.com/t5/user/viewprofilepage/user-id/9863')" class="sapUiTinyMarginBegin sapUiSmallMarginEnd"></Image>
                                </HBox>
                            </VBox>
                    </l:BlockLayoutCell>
                    <l:BlockLayoutCell
                            backgroundColorSet="ColorSet10"
                            backgroundColorShade="ShadeC"
                            title="Bio">
                                <l:VerticalLayout
                                        width="100%">
                                    <Text
                                            text="Hello, my name is Wouter Lemaire and I'm an independent SAP solution architect, developer &amp; consultant. I started more than 10 years ago when ABAP WebDynpro was like Fiori today. As soon as UI5 came around the corner, I started building Fiori applications and pushed UI5 to its limits. Since then I have been following the latest SAP Innovations which led me to Business Technology Platform (BTP) from in the early days when it still was called HANA Cloud Platform. This made me grow from a developer into an Solution Architect which I still love to comebine. As a developer and architect I focus on BTP, Fiori/WorkZone, Cloud Application Programming model, UI5 and ABAP. My goal is to share my experience as a developer and architect to help customers with:"
                                            class="sapUiSmallMarginBottom"/>
                                    <HBox>
                                        <core:Icon
                                                src="sap-icon://cloud"
                                                size="1rem"
                                                color="Default"
                                                class="sapUiTinyMarginBegin sapUiSmallMarginEnd sapUiSmallMarginTopBottom"/>
                                        <Title width="100%"
                                            level="H5"
                                            titleStyle="H5"
                                            class="sapUiSmallMarginTopBottom">
                                            <Link text="Business Technology Platform (BTP)" press=".onNavigateTo('btp')" />
                                        </Title>
                                    </HBox><HBox>
                                        <core:Icon
                                                src="sap-icon://desktop-mobile"
                                                size="1rem"
                                                color="Default"
                                                class="sapUiTinyMarginBegin sapUiSmallMarginEnd sapUiSmallMarginTopBottom"/>
                                        <Title width="100%"
                                            level="H5"
                                            titleStyle="H5"
                                            class="sapUiSmallMarginTopBottom">
                                            <Link text="Fiori Launchpad &amp; WorkZone" press=".onNavigateTo('fiori')" />
                                        </Title>
                                    </HBox><HBox>
                                        <core:Icon
                                                src="sap-icon://syntax"
                                                size="1rem"
                                                color="Default"
                                                class="sapUiTinyMarginBegin sapUiSmallMarginEnd sapUiSmallMarginTopBottom"/>
                                        <Title width="100%"
                                            level="H5"
                                            titleStyle="H5"
                                            text="Cloud Application Programming model"
                                            class="sapUiSmallMarginTopBottom">
                                            <!-- <Link text="Cloud Application Programming model" press=".onNavigateTo('cap')" /> -->
                                        </Title>
                                    </HBox><HBox>
                                        <core:Icon
                                                src="sap-icon://sap-ui5"
                                                size="1rem"
                                                color="Default"
                                                class="sapUiTinyMarginBegin sapUiSmallMarginEnd sapUiSmallMarginTopBottom"/>
                                        <Title width="100%"
                                            level="H5"
                                            text="SAPUI5 &amp; Fiori Elements"
                                            titleStyle="H5"
                                            class="sapUiSmallMarginTopBottom">
                                            <!-- <Link text="SAPUI5" press=".onNavigateTo('sapui5')" /> -->
                                        </Title>
                                    </HBox><HBox>
                                        <core:Icon
                                                src="sap-icon://syntax"
                                                size="1rem"
                                                color="Default"
                                                class="sapUiTinyMarginBegin sapUiSmallMarginEnd sapUiSmallMarginTopBottom"/>
                                        <Title width="100%"
                                            level="H5"
                                            titleStyle="H5"
                                            text="ABAP RESTful Application Programming"
                                            class="sapUiSmallMarginTopBottom">
                                            <!-- <Link text="Cloud Application Programming model" press=".onNavigateTo('cap')" /> -->
                                        </Title>
                                    </HBox>
                                </l:VerticalLayout>
